
public class s {
	
	public static void main(String [] args) {
		functionFirst();
		functionSecond();
		functionThird();
	}
	
	public static void functionFirst() {
		System.out.println("Hello World!");
	}

	public static void functionSecond() {
		 functionFirst();
		 functionFirst();
	}
	
	public static int functionThird() {
		functionSecond();
		return 0;
		
	}
}
